	box-sizing: border-box;
	text-align: center;
	background: #FAFAF3;
	padding: 50px 10%;
	margin: 0 auto;
}
.page-natural #sec03 img {
	display: block;
	margin: 0 auto;
}
.page-natural #sec03 img.sec_ttl {
	margin-bottom: 50px;
}
.page-natural #sec03 .sec03_block {
	font-size: 1.6rem;
	margin-bottom: 60px;
}
.page-natural #sec03 .sec03_block .ttl {
	font-size: 2.2rem;
	margin: 1rem 0 2rem;
}
.page-natural #sec03 img.point {
	width: 80%;
}
.page-natural #sec03 img.border {
	margin: 80px auto;
}
.page-natural #sec03 .lead {
	font-size: 1.8rem;
}
.page-natural #sec03 .tbl {
	width: 100%;
	display: table;
	margin: 50px auto;
}
.page-natural #sec03 .tbl .left {
	width: 45%;
	display: table-cell;
	vertical-align: middle;
}
.page-natural #sec03 .tbl .left .txtBox {
	width: 70%;
	font-size: 1.4rem;
	line-height: 1.8;
	margin: 0 auto;
}
.page-natural #sec03 .tbl .left .txtBox .ttl {
	font-size: 1.8rem;
	line-height: 1.4;
}
.page-natural #sec03 .tbl .right {
	width: 55%;
	display: table-cell;
	vertical-align: middle;
}
.page-natural #sec03 img.mh {
	margin-bottom: 50px;
}


/* スマホ */
@media screen and (max-width: 639px) {
	.page-natural .site-main {
		padding-top: 0;
	}
  .page-natural .page-header {
    height: auto;
	margin-top: -7.6rem;
  }
  .page-natural .sp-hide {
    display: none;
  }
  .page-natural #sec01 {
	height: auto;
	padding: 10px 0;
  }
	.page-natural #sec01 ul {
		width: 94%;
		margin: 0 auto;
	}
	.page-natural #sec01 {
		padding: 10px 0;
	}
	.page-natural #sec01 ul {
		width: 98%;
	}
	.page-natural #sec01 ul li {
		font-size: 0.8rem;
	}
	.page-natural #sec01 ul li .ttl {
		font-size: 1.0rem;
	}
	.page-natural #sec03 {
		width: 98%;
		text-align: left;
		padding: 20px 2%;
	}
	.page-natural #sec03 .sec03_block {
		font-size: 1.2rem;
	}
	.page-natural #sec03 .sec03_block .ttl {
		font-size: 1.7rem;
	}
	.page-natural #sec03 .sec03_block br {
		display: none;
	}
	.page-natural #sec03 img.border {
		margin: 30px auto;
	}
	.page-natural #sec03 .lead {
		font-size: 1.4rem;
	}
	.page-natural #sec03 .tbl {
		width: 100%;
		display: block;
		margin: 20px auto;
	}
	.page-natural #sec03 .tbl .left {
		width: 100%;
		display: block;
	}
	.page-natural #sec03 .tbl .left .txtBox {
		width: 98%;
		font-size: 0.8rem;
		line-height: 1.4;
	}
	.page-natural #sec03 .tbl .left .txtBox .ttl {
		font-size: 1.2rem;
		line-height: 1.2;
	}
	.page-natural #sec03 .tbl .right {
		width: 80%;
		display: block;
		margin: auto;
	}
	.page-natural #sec03 img.mh {
		margin-bottom: 15px;
	}
}


/* タブレット */
@media screen and (min-width: 640px) and (max-width: 959px) {
  .page-natural .tb-hide {
    display: none;
  }
	@media screen and (max-width: 767px) {
		.page-natural .site-main {
			padding-top: 0;
		}
		/*.page-natural .page-header {
			height: auto;
			margin-top: -7.6rem;
		}*/
	}
  .page-natural #sec01 {
	height: auto;
	padding: 25px 0;
  }
	.page-natural #sec01 ul {
		width: 80%;
		margin: 0 auto;
	}
	.page-natural #sec01 {
		padding: 25px 0;
	}
	.page-natural #sec01 ul {
		width: 96%;
	}
	.page-natural #sec01 ul li {
		font-size: 1.2rem;
	}
	.page-natural #sec01 ul li .ttl {
		font-size: 1.6rem;
	}
	.page-natural #sec03 {
		width: 90%;
		padding: 20px 5%;
	}
}

/* PC */
@media screen and (min-width: 960px) {
  .page-natural .pc-hide {
    display: none;
  }
}




/*! ======================================================================================= ^^
  Page modelroom
=========================================================================================== ^^ */

.page-modelroom article.content{
  padding: 0 0;
  background:#ffffff;
  color: #000000;

}
.page-modelroom .photo {
	width: 80%;
	padding: 50px 0 80px;
	margin: auto;
}
.page-modelroom .photo .bx-wrapper {
	width: 85%;
	box-sizing: border-box;
	/*background: rgba(39, 56, 80, 0.8);
	background: rgba(0, 0, 0, 0.7);*/
	padding: 0;
}
.page-modelroom .photo .custom_pager {
	width: 100%;
	margin-top: 50px;
}
.page-modelroom .photo .custom_pager a {
	width: 16%;
	display: block;
	float: left;
	margin-left: 0.8%;
	margin-bottom: 5px;
	position: relative;
}
.page-modelroom .photo .custom_pager a .mask {
	position: absolute;
	top: 0;
	left: 0;
}
.page-modelroom .photo .custom_pager a.active .mask {
	opacity: 0;
}
.page-modelroom .photo .custom_pager a:nth-child(6n+1) {
	margin-left: 0;
}


/* スマホ */
@media screen and (max-width: 639px) {
	.page-modelroom article.content {
		padding-top: 7.6rem;
	}
	.page-modelroom article {
		width: 100%;
		padding: 50px 0 80px;
		margin: auto;
	}
	.page-modelroom .photo {
		width: 100%;
		padding: 10px 0 20px;
		margin: auto;
	}
	.page-modelroom .photo .bx-wrapper {
		width: 100%;
		box-sizing: border-box;
		background: rgba(39, 56, 80, 0.8);
	}
	.page-modelroom .photo .custom_pager {
		width: 98%;
		margin: 10px auto 0;
	}
}


/* タブレット */
@media screen and (min-width: 640px) and (max-width: 959px) {
	@media screen and (max-width: 767px) {
		.page-modelroom article.content {
			padding-top: 7.6rem;
		}
	}
	.page-modelroom .photo {
		width: 100%;
		padding: 20px 0 20px;
		margin: auto;
	}
	.page-modelroom .photo .custom_pager {
		width: 98%;
		margin: 20px auto 0;
	}
}

/* PC */
@media screen and (min-width: 960px) {
  .page-plan .pc-hide {
    display: none;
  }
}

.content #sec01 ul .disable a {
	cursor: default;
	pointer-events:none;
	}

.content #sec01 ul .disable a:hover {
		background: -webkit-gradient(linear, left top, left bottom, from(#3C2908), to(#170F03));
		-ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr='#3C2908', endColorstr='#170F03', GradientType=0)";
  background: -ms-linear-gradient(top, #3C2908, #170F03);
  background: -moz-linear-gradient(top, #3C2908, #170F03);　/* Firefox用 */
		position: relative;
		z-index: 1000;
	}

.content #sec01 ul .disable img {
		filter: alpha(opacity=20);
		-ms-filter: "alpha(opacity=20)";
}


