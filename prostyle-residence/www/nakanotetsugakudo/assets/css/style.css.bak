/*! ======================================================================================= ^^
  Layout Common
=========================================================================================== ^^ */
body.page {
  padding-top: 7.6rem;
  background-color: #3c3935;
}
body.page.scroll-fix {
  padding-top: 5rem;
}

@media screen and (max-width: 767px) {
  body.home {
    padding-top: 7.6rem;
  }
  body.scroll-fix {
    padding-top: 5rem;
  }
}
.iScrollVerticalScrollbar.iScrollLoneScrollbar {
  width: 0 !important;
}

.clearfix::after {
  content: '\0020';
  display: block;
  visibility: hidden;
  clear: both;
}

/*! ======================================================================================= ^^
  Layout Header
=========================================================================================== ^^ */
.site-header {
  width: 100%;
  height: 7.6rem;
  background-color: #fff;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10000;
  -moz-transition: all 0.5s;
  -o-transition: all 0.5s;
  -webkit-transition: all 0.5s;
  transition: all 0.5s;
}
.site-header .inner {
  padding-left: 2rem;
  position: relative;
  *zoom: 1;
}
.site-header .inner:after {
  content: "\0020";
  display: block;
  height: 0;
  clear: both;
  overflow: hidden;
  visibility: hidden;
}
.site-header .site-logo {
  max-width: 20.4rem;
  padding-top: 2rem;
  margin: 0;
  float: left;
  -moz-transition: all 0.5s;
  -o-transition: all 0.5s;
  -webkit-transition: all 0.5s;
  transition: all 0.5s;
}
.site-header .sp-contents {
  width: 7.8rem;
  padding-top: 2rem;
  font-size: 0;
  float: right;
  display: none;
  -moz-transition: all 0.5s;
  -o-transition: all 0.5s;
  -webkit-transition: all 0.5s;
  transition: all 0.5s;
}
.site-header .sp-contents .tel {
  width: 2.8rem;
  margin-right: 2rem;
  display: inline-block;
  vertical-align: middle;
}
.site-header .sp-contents .toggle-menu {
  width: 3rem;
  display: inline-block;
  vertical-align: middle;
}
.scroll-fix .site-header {
  height: 5rem;
}
.scroll-fix .site-header .site-logo {
  padding-top: 0.8rem;
}
.scroll-fix .site-header .sp-contents {
  padding-top: 0.8rem;
}

@media screen and (max-width: 767px) {
  .scroll-fix.scroll-down .site-header {
    top: -6rem;
  }
  .site-header .inner {
    padding: 0 1rem;
  }
  .site-header .sp-contents {
    display: block;
  }
}
@media print {
  .site-header {
    position: static;
  }
  .scroll-fix .site-header {
    height: 7.6rem;
  }
}
/*! ======================================================================================= ^^
  Layout Footer
=========================================================================================== ^^ */
.site-footer {
  height: auto !important;
  background-color: #4d4344;
  display: block !important;
}
.site-footer .fp-tableCell {
  height: auto !important;
  display: block !important;
}
.site-footer .bottom-text {
  width: 100%;
  height: 12rem;
  background-color: #4d4344;
  font-size: 3rem;
  color: #fff;
  line-height: 12rem;
  text-align: center;
}
.site-footer .bottom-text::before {
  width: 1px;
  height: 100%;
  content: '\0020';
  display: inline-block;
  vertical-align: middle;
}
.site-footer .bottom-text .num {
  padding: 0 0.5rem;
  font-size: 4.8rem;
  color: #e2e42e;
}
.site-footer .bottom-text br {
  display: none;
}
.site-footer .bottom-text .pc-hide {
  display: none;
}
.site-footer .bottom-text .sp-hide {
/*  max-width: 80.4rem; */
  max-width: 200rem;
/*  width: 62.8125%; */
  width: 70%;
  display: inline-block;
  vertical-align: middle;
}
.site-footer .bottom-content {
  width: 100%;
  padding: 2rem 4rem;
  background-color: #3c3935;
  *zoom: 1;
}
.site-footer .bottom-content:after {
  content: "\0020";
  display: block;
  height: 0;
  clear: both;
  overflow: hidden;
  visibility: hidden;
}
.site-footer .bottom-content .fp-tableCell {
  height: auto !important;
  display: block !important;
}
.site-footer .bottom-content .bottom-info {
  max-width: 88rem;
  width: 73.33333%;
  font-size: 0;
  float: left;
}
.site-footer .bottom-content .bottom-info dt {
  width: 9.09091%;
  font-size: 1.4rem;
  color: #b48856;
  display: inline-block;
  vertical-align: middle;
}
.site-footer .bottom-content .bottom-info .scroll {
  width: 90.90909%;
  height: 6rem;
  display: inline-block;
  vertical-align: middle;
  overflow: auto;
}
.site-footer .bottom-content .bottom-info .scroll ul li {
  font-size: 2.4rem;
  color: #fff;
  line-height: 2.4;
}
.site-footer .bottom-content .buttons {
  max-width: 26.4rem;
  width: 23.66667%;
  font-size: 0;
  float: right;
}
.site-footer .bottom-content .buttons li {
  width: 46.47887%;
  height: 6rem;
  margin-left: 7.04225%;
  display: inline-block;
}
.site-footer .bottom-content .buttons li a {
  width: 100%;
  height: 100%;
  font-size: 1.8rem;
  color: #fff;
  text-align: center;
  text-decoration: none;
  display: block;
}
.site-footer .bottom-content .buttons li a::before {
  width: 1px;
  height: 100%;
  content: '\0020';
  display: inline-block;
  vertical-align: middle;
}
.site-footer .bottom-content .buttons li.request {
  margin-left: 0;
}
.site-footer .bottom-content .buttons li.request a {
  background: url('data:image/svg+xml;base64,');
  background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #a45a2e), color-stop(100%, #743d1f));
  background: -moz-linear-gradient(#a45a2e, #743d1f);
  background: -webkit-linear-gradient(#a45a2e, #743d1f);
  background: linear-gradient(#a45a2e, #743d1f);
}
.site-footer .bottom-content .buttons li.request a:hover {
  background: url('data:image/svg+xml;base64,');
  background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #743d1f), color-stop(100%, #a45a2e));
  background: -moz-linear-gradient(#743d1f, #a45a2e);
  background: -webkit-linear-gradient(#743d1f, #a45a2e);
